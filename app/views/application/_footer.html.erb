<footer class="blog-footer">
  <p>Blog template built for <a href="http://getbootstrap.com">Bootstrap</a> by <a href="https://twitter.com/mdo">@mdo</a>. And copied by
    <a href="<%= root_path %>">me</a>! :)
  </p>
  <p>
    <a class="btn btn-default" href="#">Back to top</a>
  </p>
    <div class="btn-group" role="group" >
      <%= link_to 'RSS', feed_path, class: 'btn btn-warning' %>
      <% if user_signed_in?  %>
        <%= link_to t('sign_out'), destroy_user_session_path, method: :delete, class: 'btn btn-danger' %>
        <%= # link_to t('edit_user'), edit_user_registration_path if edit_user_registration_path
    %>
      <% else %>
        <%= link_to t('sign_in'), new_user_session_path, class: 'btn btn-info' %>
      <% end %>
      <%= link_to 'Sitemap.xml', sitemap_path, class: 'btn btn-default' %>
    </div>
</footer>
<%= render 'application/ga' unless user_signed_in? %>